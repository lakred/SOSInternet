{
  "RouterSettings": {
    "Url": "http://192.168.1.1", //Default: http://192.168.1.1
    "Username": "admin",
    "Password": "password",
    "UseEncryption": false
  },
  "ConnectionSettings": {
    "CheckIntervalSeconds": 15,
    "RetryAttemptsBeforeReboot": 1,
    "WaitAfterRebootSeconds": 120,  
    "PingTargets": [
      "8.8.8.8",
      "1.1.1.1",
      "google.com"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    },
    "FileLogging": {
      "Path": "logs",
      "RetentionDays": 30 // Default 30 days
    }
  }
}